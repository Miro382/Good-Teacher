<Window x:Class="Good_Teacher.Windows.Dialogs.DWindow_AddFromWeb"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Good_Teacher.Windows.Dialogs"
        xmlns:strings="clr-namespace:Good_Teacher.Strings"
        mc:Ignorable="d"
        Title="{x:Static strings:ResStrings.WebAddress}" Height="150" Width="250" Icon="/Good Teacher;component/Resources/EditValues/FindWeb.png" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">

    <StackPanel Margin="10,10,10,0">
        <Label  Content="{x:Static strings:ResStrings.WebAddress}"/>
        <TextBox x:Name="TextBox_Address" Height="23" Grid.Column="0"  Text=""/>

        <Button Margin="0,20,0,0" Content="{x:Static strings:ResStrings.OK}" Width="60" HorizontalAlignment="Center" Click="ButtonAdd_Click" />
    </StackPanel>

</Window>
