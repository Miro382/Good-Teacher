<Window x:Class="Good_Teacher.Windows.Dialogs.DWindow_AddFromClipboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:strings ="clr-namespace:Good_Teacher.Strings"
        xmlns:local="clr-namespace:Good_Teacher.Windows.Dialogs"
        mc:Ignorable="d"
        Title="{x:Static strings:ResStrings.FromClipboard}" Height="420" Width="550" WindowStartupLocation="CenterOwner" Icon="/Good Teacher;component/Resources/Controls/image.png">
    <Grid>
        <Image x:Name="img" Margin="0,0,0,50" RenderOptions.BitmapScalingMode="Fant" />
        <Button Click="ButtonAdd_Click" Content="{x:Static strings:ResStrings.Add}" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="10,0,0,8" Width="80" FontSize="16" Height="32"/>
        <Button Click="ButtonCancel_Click" Content="{x:Static strings:ResStrings.Cancel}" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,10,8" Width="80" FontSize="16" Height="32"/>

    </Grid>
</Window>
